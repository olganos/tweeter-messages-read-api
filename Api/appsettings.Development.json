{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    }
  },
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:8007/?readPreference=primary&ssl=false",
    "DbName": "tweeter-messages",
    "DbTweetCollectionName": "tweet",
    "DbReplyCollectionName": "reply",
    "DbUserCollectionName": "user",
    "DbLikeCollectionName": "like"
  },
  "IdentityServer": {
    "Uri": "https://localhost:8003"
  },
  "ElasticConfiguration": {
    "Uri": "http://tweeter-app-logstash.westeurope.azurecontainer.io:9200"
  }
}
